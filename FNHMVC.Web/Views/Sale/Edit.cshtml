@model FNHMVC.Web.ViewModels.SaleFormUpdateDataModel

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/shared/_LayoutPrincipal.cshtml";
}

<legend>
    <div style="text-align: center;">
        <h2>Editar publicación <small>
            <h5>Requiere aprobación</h5>
        </small></h2>

    </div>
</legend>


@*<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>*@

@using (Html.BeginForm("Edit", "Sale"))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    <fieldset>
        @Html.Partial("_Edit", Model)


    </fieldset>
    <div class="form-actions">
        <button type="submit" class="btn btn-primary">Editar</button>
        @Html.ActionLink("Cancelar", "Index", "Sale", new { @class = "btn " })
    </div>
}


